{
  "expo": {
    "name": "YouMeet",
    "description": "This project is really great.",
    "slug": "YouMeet",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "wpam.youmeet.front"
    },
    "android": {
      "package": "wpam.youmeet.front",
      "versionCode": 1,
      "permissions" : [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config" : {
        "googleMaps": {
          "apiKey": "xxx"
        }
      }
    },
    "facebookScheme": "fb269068230632241"
  }
}
